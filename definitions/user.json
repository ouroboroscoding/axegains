{
	"__storage__": {
		"server": "default",
		"db": "axegains"
	},
	"__name__": "user",
	"_id": {"__type__":"uuid", "__optional__":true},
	"_created": {"__type__":"timestamp"},
	"name": {"__type__":"string", "__regex__":"^[a-zA-Z0-9_-]( ?[a-zA-Z0-9_-]+)*$"},
	"passwd": {"__type__":"string", "__regex__":"^[0-9a-fA-F]{72}$"},
	"active": {"__type__":"bool"},
	"verified": {"__type__":"bool"},
	"email": {"__type__":"string"}
}
